{
  "name": "treasure-key-bot",
  "version": "1.0.6",
  "description": "Treasure key bot to help players play the game automatically",
  "main": "./dist/main.js",
  "scripts": {
    "generate": "hardhat compile && hardhat typechain",
    "dev:electron": "NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
    "dev:react": "NODE_ENV=development webpack-dev-server --config webpack.react.config.js --mode development",
    "build:electron": "NODE_ENV=production webpack --config webpack.electron.config.js --mode production",
    "build:react": "NODE_ENV=production webpack --config webpack.react.config.js --mode production",
    "build": "npm run build:electron && npm run build:react",
    "pack": "electron-builder --dir",
    "dist": "yarn dist:mac && yarn dist:win && yarn dist:linux",
    "dist:mac": "electron-builder --mac",
    "dist:win": "electron-builder --win",
    "dist:linux": "electron-builder --linux",
    "package-linux": "electron-packager .  treasure-key-bot --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "package-debian": "electron-installer-debian --src release-builds/treasure-key-bot-linux-x64/ --arch amd64 --config debian.json",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "bump": "npm version patch -m \"bump version to %s\""
  },
  "keywords": [],
  "author": "shark",
  "license": "ISC",
  "build": {
    "files": [
      "dist/",
      "node_modules/",
      "package.json",
      "!builds/*",
      "!patches/*",
      "!packages/*",
      "!outputs/*"
    ],
    "productName": "Treasure Key Bot",
    "appId": "com.treasurekeybet.bot",
    "directories": {
      "buildResources": "resources",
      "output": "outputs"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-etherscan": "^2.1.2",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/cli": "^2.8.2",
    "@openzeppelin/contract-loader": "^0.6.1",
    "@openzeppelin/contracts": "3.4.1",
    "@openzeppelin/contracts-upgradeable": "3.4.1",
    "@openzeppelin/hardhat-upgrades": "^1.8.2",
    "@openzeppelin/test-environment": "^0.1.4",
    "@openzeppelin/test-helpers": "^0.5.6",
    "@typechain/ethers-v5": "^7.0.0",
    "@typechain/hardhat": "^2.0.1",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "@types/node-fetch": "^2.5.7",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "electron": "^8.2.2",
    "electron-builder": "^22.7.0",
    "electron-installer-debian": "^3.1.0",
    "electron-packager": "^15.1.0",
    "ethereum-waffle": "^3.2.1",
    "ethers": "^5.0.24",
    "ganache-core": "^2.13.0",
    "html-webpack-plugin": "^4.2.0",
    "json-loader": "^0.5.7",
    "mocha": "^8.1.3",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.0.0",
    "typechain": "^5.0.0",
    "typescript": "^4.3.2",
    "web3": "^1.3.1",
    "web3-utils": "^1.2.11",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@pancakeswap/pancake-swap-lib": "^0.0.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "antd": "^4.7.0",
    "css-loader": "^5.0.0",
    "electron-json-config": "^1.5.3",
    "electron-settings": "^4.0.2",
    "hardhat": "^2.0.6",
    "hardhat-typechain": "^0.3.5",
    "node-fetch": "^2.6.1",
    "pantherswap-peripheral": "^1.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^2.0.0"
  }
}
